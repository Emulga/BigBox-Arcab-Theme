				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="{Binding ElementName=Prop_ThumbnailRecentGames_W, Path=Text}" />
				</Grid.ColumnDefinitions>
				
				<transitions:TransitionPresenter Grid.Row="0" Grid.Column="0" VerticalAlignment="Top"
					TransitionSelector="{Binding BottomBoxesTransitionSelector}" Content="{Binding BottomBoxesView}" >
				</transitions:TransitionPresenter>
					
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_ThumbnailRecentGames_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_ThumbnailRecentGames_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_ThumbnailRecentGames_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_ThumbnailRecentGames_CenterY, Path=Text}" Angle="{Binding ElementName=Prop_ThumbnailRecentGames_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_ThumbnailRecentGames_X, Path=Text}" CenterY="{Binding ElementName=Prop_ThumbnailRecentGames_Y, Path=Text}" AngleX="{Binding ElementName=Prop_ThumbnailRecentGames_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_ThumbnailRecentGames_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Marquee Background -->
			<Grid x:Name="AnimatedGIF1" Panel.ZIndex="70" >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				<Image x:Name="AnimatedGIF1img" Grid.Row="0" Grid.Column="0" gif:ImageBehavior.AnimatedSource="{Binding ElementName=Prop_AnimatedGIF1_FileName, Path=Text}"
					Width="{Binding ElementName=Prop_AnimatedGIF1_W, Path=Text}" Height="{Binding ElementName=Prop_AnimatedGIF1_H, Path=Text}"  Opacity="1.0" Stretch="Uniform"
					HorizontalAlignment="Left" VerticalAlignment="Top"
					RenderOptions.BitmapScalingMode="HighQuality"
					RenderTransformOrigin=".5,.5" >
					<Image.RenderTransform>
						<TransformGroup>
							<RotateTransform Angle="0" />
							<ScaleTransform ScaleX="1" />
							<ScaleTransform ScaleY="1" />
						</TransformGroup>
					</Image.RenderTransform>
						
				</Image>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_AnimatedGIF1_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_AnimatedGIF1_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_AnimatedGIF1_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_AnimatedGIF1_CenterY, Path=Text}"  Angle="{Binding ElementName=Prop_AnimatedGIF1_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_AnimatedGIF1_X, Path=Text}" CenterY="{Binding ElementName=Prop_AnimatedGIF1_Y, Path=Text}" AngleX="{Binding ElementName=Prop_AnimatedGIF1_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_AnimatedGIF1_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Marquee Text Scroller -->
			<Grid Panel.ZIndex="80" >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				
				<Rectangle VerticalAlignment="Top" Grid.Row="0" Grid.Column="0" Opacity="0" Fill="{Binding Source=Black, Converter={StaticResource TextToBrush}}" Height="{Binding ElementName=Prop_TextScroller1_H, Path=Text}" Width="{Binding ElementName=Prop_TextScroller1_W, Path=Text}" >
				</Rectangle>
				
				<Grid x:Name="TextScroller1_Horizontal">
					<Viewbox Grid.Row="0" Grid.Column="0" >
						<DockPanel Height="{Binding ElementName=Prop_TextScroller1_H, Path=Text}"  Width="{Binding ElementName=Prop_TextScroller1_W, Path=Text}" HorizontalAlignment="Stretch" VerticalAlignment="Center"  >
							<Canvas Name="TextScroller1_ScrollingTextBlockCanvas" Canvas.Left="{Binding ElementName=Prop_TextScroller1_X, Path=Text}" ClipToBounds="True"  >
								<HelperControl:ScrollableTextBlock x:Name="TextScroller1_HorizScroller" TextWrapping="Wrap" FontFamily="LAUNCHBOX_ROOT_FOLDER/Themes/Arcab/Fonts/The Led Display St.ttf#The Led Display St" FontSize="{Binding ElementName=Prop_TextScroller1_H, Path=Text, Converter={StaticResource SetTextFontSize}}" Foreground="{Binding Source=#FF8080, Converter={StaticResource TextToBrush}}" Opacity="1.0" ScrollSpeed="100" Margin="{Binding ElementName=Prop_TextScroller1_V, Path=Text}"
									Text="{Binding Path=SelectedPlatform.Name}" >
									
								</HelperControl:ScrollableTextBlock>
							</Canvas>
						</DockPanel>
					</Viewbox>
				</Grid>
				
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_TextScroller1_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_TextScroller1_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_TextScroller1_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_TextScroller1_CenterY, Path=Text}"  Angle="{Binding ElementName=Prop_TextScroller1_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_TextScroller1_X, Path=Text}" CenterY="{Binding ElementName=Prop_TextScroller1_Y, Path=Text}" AngleX="{Binding ElementName=Prop_TextScroller1_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_TextScroller1_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Cab -->
			<Grid x:Name="Image1" Panel.ZIndex="90" >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				<Image x:Name="Image1img" Grid.Row="0" Grid.Column="0" Source="{Binding ElementName=Prop_Image1_FileName, Path=Text}"
					Width="{Binding ElementName=Canvas, Path=ActualWidth}"
					Height="{Binding ElementName=Canvas, Path=ActualHeight}"
					Stretch="Uniform" HorizontalAlignment="Right"
					VerticalAlignment="Center" RenderOptions.BitmapScalingMode="HighQuality" Opacity="1.0"
					RenderTransformOrigin=".5,.5" >
					<Image.RenderTransform>
						<TransformGroup>
							<RotateTransform Angle="0" />
							<ScaleTransform ScaleX="1" />
							<ScaleTransform ScaleY="1" />
						</TransformGroup>
					</Image.RenderTransform>
						
						
				</Image>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_Image1_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_Image1_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_Image1_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_Image1_CenterY, Path=Text}"  Angle="{Binding ElementName=Prop_Image1_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_Image1_X, Path=Text}" CenterY="{Binding ElementName=Prop_Image1_Y, Path=Text}" AngleX="{Binding ElementName=Prop_Image1_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_Image1_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Text Games -->
			<Grid x:Name="Text1" Panel.ZIndex="100"  >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				<Viewbox Grid.Row="0" Grid.Column="0" Height="Auto">
					<Grid Height="{Binding ElementName=Prop_Text1_H, Path=Text}" >
						<TextBlock x:Name="Text1text"
							Width="{Binding ElementName=Prop_Text1_W, Path=Text}"
							Foreground="{Binding Source=#FF4c4006, Converter={StaticResource TextToBrush}}" Opacity="1.0"
							FontFamily="LAUNCHBOX_ROOT_FOLDER/Themes/Arcab/Fonts/digital-7.ttf#Digital-7" FontSize="{Binding ElementName=Prop_Text1_FontSize, Path=Text}" FontStyle="Normal" FontWeight="Normal"
							VerticalAlignment="Top" TextAlignment="Left" TextWrapping="NoWrap" TextTrimming="None"
							Text="GAMES:" >
							
						</TextBlock>
					</Grid>
				</Viewbox>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_Text1_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_Text1_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_Text1_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_Text1_CenterY, Path=Text}"  Angle="{Binding ElementName=Prop_Text1_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_Text1_X, Path=Text}" CenterY="{Binding ElementName=Prop_Text1_Y, Path=Text}" AngleX="{Binding ElementName=Prop_Text1_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_Text1_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Text Game Count -->
			<Grid x:Name="PlatformTotalGameCount1" Panel.ZIndex="110"  >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				<Viewbox Grid.Row="0" Grid.Column="0" Height="Auto">
					<Grid Height="{Binding ElementName=Prop_PlatformTotalGameCount1_H, Path=Text}" >
						<TextBlock x:Name="variablePlatformTotalGameCount1"
							Width="{Binding ElementName=Prop_PlatformTotalGameCount1_W, Path=Text}"
							Foreground="{Binding Source=#FF4C4006, Converter={StaticResource TextToBrush}}" Opacity="1.0"
							FontFamily="LAUNCHBOX_ROOT_FOLDER/Themes/Arcab/Fonts/digital-7.ttf#Digital-7" FontSize="{Binding ElementName=Prop_PlatformTotalGameCount1_FontSize, Path=Text}" FontStyle="Normal" FontWeight="Normal"
							TextAlignment="Left" TextWrapping="NoWrap" TextTrimming="None"
							Text="{Binding GamesCount}" VerticalAlignment="Top">
							
						</TextBlock>
					</Grid>
				</Viewbox>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_PlatformTotalGameCount1_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_PlatformTotalGameCount1_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_PlatformTotalGameCount1_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_PlatformTotalGameCount1_CenterY, Path=Text}"  Angle="{Binding ElementName=Prop_PlatformTotalGameCount1_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_PlatformTotalGameCount1_X, Path=Text}" CenterY="{Binding ElementName=Prop_PlatformTotalGameCount1_Y, Path=Text}" AngleX="{Binding ElementName=Prop_PlatformTotalGameCount1_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_PlatformTotalGameCount1_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
		</Grid>
	</Grid>
</UserControl>