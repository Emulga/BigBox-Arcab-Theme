					<Image x:Name="PlayModeImage1" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality"
						Source="{Binding ElementName=PlayModeImagePath1,Path=Text}" Margin="{Binding ElementName=PlayModeImageHorizOnly,Path=Text}" >
						<Image.Style>
							<Style TargetType="Image">
								<Setter Property="Visibility" Value="Visible" />
								<Style.Triggers>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage1, Path=Source}" Value="{x:Null}" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage1, Path=Width}" Value="0" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
								</Style.Triggers>
							</Style>
						</Image.Style>
						
					</Image>
					<!-- Image 2 -->
					<Image x:Name="PlayModeImage2" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality"
						Source="{Binding ElementName=PlayModeImagePath2,Path=Text}" Margin="{Binding ElementName=PlayModeImageHorizTopOnly,Path=Text}" >
						<Image.Style>
							<Style TargetType="Image">
								<Setter Property="Visibility" Value="Visible" />
								<Style.Triggers>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage2, Path=Source}" Value="{x:Null}" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage2, Path=Width}" Value="0" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
								</Style.Triggers>
							</Style>
						</Image.Style>
						
					</Image>
					<!-- Image 3 -->
					<Image x:Name="PlayModeImage3" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality"
						Source="{Binding ElementName=PlayModeImagePath3,Path=Text}" Margin="{Binding ElementName=PlayModeImageHorizTopOnly,Path=Text}" >
						<Image.Style>
							<Style TargetType="Image">
								<Setter Property="Visibility" Value="Visible" />
								<Style.Triggers>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage3, Path=Source}" Value="{x:Null}" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage3, Path=Width}" Value="0" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
								</Style.Triggers>
							</Style>
						</Image.Style>
						
					</Image>
					<!-- Image 4 -->
					<Image x:Name="PlayModeImage4" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality"
						Source="{Binding ElementName=PlayModeImagePath4,Path=Text}" Margin="{Binding ElementName=PlayModeImageHorizTopOnly,Path=Text}" >
						<Image.Style>
							<Style TargetType="Image">
								<Setter Property="Visibility" Value="Visible" />
								<Style.Triggers>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage4, Path=Source}" Value="{x:Null}" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
									<DataTrigger Binding="{Binding ElementName=PlayModeImage4, Path=Width}" Value="0" >
										<Setter Property="Visibility" Value="Collapsed" />
									</DataTrigger>
								</Style.Triggers>
							</Style>
						</Image.Style>
						
					</Image>
				</StackPanel>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_PlayModeImage_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_PlayModeImage_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_PlayModeImage_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_PlayModeImage_CenterY, Path=Text}" Angle="{Binding ElementName=Prop_PlayModeImage_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_PlayModeImage_X, Path=Text}" CenterY="{Binding ElementName=Prop_PlayModeImage_Y, Path=Text}" AngleX="{Binding ElementName=Prop_PlayModeImage_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_PlayModeImage_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Icon Community Rating -->
			<Grid x:Name="CommunityRating" Panel.ZIndex="130" >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				<TextBlock x:Name="CommunityRatingIMG" Visibility="Collapsed" >
					<TextBlock.Text>
						<MultiBinding StringFormat="{}{0}/{1:F1}.png">
							<Binding Path="." Source="pack://siteoforigin:,,,/Themes/Arcab/Media/Rating"/>
							<Binding Path="ActiveGame.CommunityStarRating" />
						</MultiBinding>
					</TextBlock.Text>
				</TextBlock>
				<Image x:Name="CommunityRatingIMAGE" Grid.Row="0" Grid.Column="0" Width="{Binding ElementName=Prop_CommunityRating_W, Path=Text}" Height="{Binding ElementName=Prop_CommunityRating_H, Path=Text}" Opacity="1" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality"
					HorizontalAlignment="Center" VerticalAlignment="Center"
					Source="{Binding ElementName=CommunityRatingIMG, Path=Text}">
					
				</Image>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_CommunityRating_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_CommunityRating_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_CommunityRating_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_CommunityRating_CenterY, Path=Text}" Angle="{Binding ElementName=Prop_CommunityRating_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_CommunityRating_X, Path=Text}" CenterY="{Binding ElementName=Prop_CommunityRating_Y, Path=Text}" AngleX="{Binding ElementName=Prop_CommunityRating_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_CommunityRating_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Icon Broken -->
			<Grid x:Name="Broken" Grid.Row="0" Grid.RowSpan="7" Grid.Column="0" Grid.ColumnSpan="9" Panel.ZIndex="140" >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				
				<TextBlock x:Name="Brokenimgpath" Visibility="Collapsed" >
					<TextBlock.Text>
						<MultiBinding Converter="{StaticResource ReturnTrueOrFalseValue}">
							<Binding Path="ActiveGame.Broken" />
							<Binding Path="." Source="pack://siteoforigin:,,,/Themes/Arcab/Media/Images\broken.png"/>
							<Binding Path="." Source="pack://siteoforigin:,,,/Themes/Arcab/Media/Images\"/>
						</MultiBinding>
					</TextBlock.Text>
				</TextBlock>
				<Image x:Name="Brokenimg" Grid.Row="0" Grid.Column="0" Width="{Binding ElementName=Prop_Broken_W, Path=Text}" Height="{Binding ElementName=Prop_Broken_H, Path=Text}" Opacity="1.0" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality" HorizontalAlignment="Center" VerticalAlignment="Center"
					Source="{Binding ElementName=Brokenimgpath, Path=Text}">
					
				</Image>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_Broken_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_Broken_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_Broken_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_Broken_CenterY, Path=Text}" Angle="{Binding ElementName=Prop_Broken_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_Broken_X, Path=Text}" CenterY="{Binding ElementName=Prop_Broken_Y, Path=Text}" AngleX="{Binding ElementName=Prop_Broken_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_Broken_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Icon Completed -->
			<Grid x:Name="Completed" Grid.Row="0" Grid.RowSpan="7" Grid.Column="0" Grid.ColumnSpan="9" Panel.ZIndex="150" >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				
				<TextBlock x:Name="COMPLETEDimgpath" Visibility="Collapsed" >
					<TextBlock.Text>
						<MultiBinding Converter="{StaticResource ReturnTrueOrFalseValue}">
							<Binding Path="ActiveGame.Completed" />
							<Binding Path="." Source="pack://siteoforigin:,,,/Themes/Arcab/Media/Images\completed.png"/>
							<Binding Path="." Source="pack://siteoforigin:,,,/Themes/Arcab/Media/Images\"/>
						</MultiBinding>
					</TextBlock.Text>
				</TextBlock>
				<Image x:Name="COMPLETEDimg" Grid.Row="0" Grid.Column="0" Width="{Binding ElementName=Prop_Completed_W, Path=Text}" Height="{Binding ElementName=Prop_Completed_H, Path=Text}" Opacity="1.0" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality" HorizontalAlignment="Center" VerticalAlignment="Center"
					Source="{Binding ElementName=COMPLETEDimgpath, Path=Text}">
					
				</Image>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_Completed_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_Completed_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_Completed_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_Completed_CenterY, Path=Text}" Angle="{Binding ElementName=Prop_Completed_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_Completed_X, Path=Text}" CenterY="{Binding ElementName=Prop_Completed_Y, Path=Text}" AngleX="{Binding ElementName=Prop_Completed_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_Completed_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Icon Favorite -->
			<Grid x:Name="Favorite" Grid.Row="0" Grid.RowSpan="7" Grid.Column="0" Grid.ColumnSpan="9" Panel.ZIndex="160" >
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto" />
				</Grid.ColumnDefinitions>
				
				<TextBlock x:Name="FAVORITEimgpath" Visibility="Collapsed" >
					<TextBlock.Text>
						<MultiBinding Converter="{StaticResource ReturnTrueOrFalseValue}">
							<Binding Path="ActiveGame.Favorite" />
							<Binding Path="." Source="pack://siteoforigin:,,,/Themes/Arcab/Media/Images\favorite_yes.png"/>
							<Binding Path="." Source="pack://siteoforigin:,,,/Themes/Arcab/Media/Images\favorite_no.png"/>
						</MultiBinding>
					</TextBlock.Text>
				</TextBlock>
				<Image x:Name="FAVORITEimg" Grid.Row="0" Grid.Column="0" Width="{Binding ElementName=Prop_Favorite_W, Path=Text}" Height="{Binding ElementName=Prop_Favorite_H, Path=Text}" Opacity="1.0" Stretch="Uniform" RenderOptions.BitmapScalingMode="HighQuality" HorizontalAlignment="Center" VerticalAlignment="Center"
					Source="{Binding ElementName=FAVORITEimgpath, Path=Text}">
					
				</Image>
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_Favorite_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_Favorite_Y, Path=Text}" />
						<RotateTransform CenterX="{Binding ElementName=Prop_Favorite_CenterX, Path=Text}" CenterY="{Binding ElementName=Prop_Favorite_CenterY, Path=Text}" Angle="{Binding ElementName=Prop_Favorite_Angle, Path=Text}" />
						<SkewTransform   CenterX="{Binding ElementName=Prop_Favorite_X, Path=Text}" CenterY="{Binding ElementName=Prop_Favorite_Y, Path=Text}" AngleX="{Binding ElementName=Prop_Favorite_SkewAngleX, Path=Text}" AngleY="{Binding ElementName=Prop_Favorite_SkewAngleY, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Game Wheel Index -->
			<Grid x:Name="GameWheelIndex" Panel.ZIndex="170"  >
				<Grid.RowDefinitions>
					<RowDefinition  Height="{Binding ElementName=Prop_GameWheelIndex_H, Path=Text}" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="{Binding ElementName=Prop_GameWheelIndex_W, Path=Text}" />
				</Grid.ColumnDefinitions>
				
				<!-- ALPHANUMERIC BAR -->
				<ListBox Name="Index" Grid.Row="0" Grid.Column="0" Style="{DynamicResource HorizontalListBoxStyle}" Visibility="{Binding IndexVisibility}" HorizontalAlignment="Left" VerticalAlignment="Top" >
					<i:Interaction.Triggers>
						<i:EventTrigger EventName="MouseDoubleClick">
							<cal:ActionMessage MethodName="OnEnter" />
						</i:EventTrigger>
					</i:Interaction.Triggers>
				</ListBox>
					
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_GameWheelIndex_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_GameWheelIndex_Y, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
			
			<!-- Wheel -->
			<Grid x:Name="Wheel" Panel.ZIndex="180"  >
				<Grid.RowDefinitions>
					<RowDefinition  Height="{Binding ElementName=Prop_Wheel_H, Path=Text}" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="{Binding ElementName=Prop_Wheel_W, Path=Text}" />
				</Grid.ColumnDefinitions>
				
				<coverFlow:FlowControl x:Name="FlowControl" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" CoverFactory="{Binding CoverFactory}"
					
					Opacity="1.0" ImageType="Clear Logo" CurveAmount="-2" VisibleCount="16" PageSize="6"
					CameraZPosition="2.3" ItemZPosition="0.7" SelectedItemZPosition="1.1" Spacing="0.9" RotationAmount="12"  >
					
				</coverFlow:FlowControl>
				
				<Grid.RenderTransform>
					<TransformGroup>
						<TranslateTransform X="{Binding ElementName=Prop_Wheel_X, Path=Text}" />
						<TranslateTransform Y="{Binding ElementName=Prop_Wheel_Y, Path=Text}" />
					</TransformGroup>
				</Grid.RenderTransform>
			</Grid>
		</Grid>
	</Grid>
</UserControl>